<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane styleClass="web-view" stylesheets="@../css/Appbar.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.AppbarController">
   <top>
      <AnchorPane fx:id="appbar" prefHeight="200.0" prefWidth="200.0" styleClass="appbar" stylesheets="@../css/Appbar.css" BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="titlebar" prefHeight="100.0" prefWidth="200.0" styleClass="titlebar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="0.0" />
            <HBox fx:id="iconbar" prefHeight="100.0" prefWidth="200.0" styleClass="iconbar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </top>
</BorderPane>
